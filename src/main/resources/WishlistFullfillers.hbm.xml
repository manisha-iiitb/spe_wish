<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                         "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	 <class name="org.acms.WishlistService.model.WishlistFullfillers" table="WishlistFullfillers">
	 
      <meta attribute = "class-description">
         This class contains the fullfillers for the wishlist. 
      </meta>
      
      <id name = "id" type = "int" column = "id">
         <generator class="native"/>
      </id>
      
      <property name = "fullfiller_id" column = "fullfiller_id" type = "string"/>
      <property name = "wishlist_id" column = "wishlist_id" type = "int"/>
     
	</class>
	
</hibernate-mapping>