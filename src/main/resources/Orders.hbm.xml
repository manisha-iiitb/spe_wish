<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                         "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	 <class name="org.acms.WishlistService.model.Orders" table="Orders">
	 
      <meta attribute = "class-description">
         This class contains the details of the orders. 
      </meta>
      
      <id name = "order_id" type = "int" column = "order_id">
         <generator class="native"/>
      </id>
      
      <property name = "wishlistfullfiller_id" column = "wishlistfullfiller_id" type = "int"/>
     
	</class>
	
</hibernate-mapping>