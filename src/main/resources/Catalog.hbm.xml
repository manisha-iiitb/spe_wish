<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                         "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	 <class name="org.acms.WishlistService.model.Catalog" table="Catalog">
	 
      <meta attribute = "class-description">
         This class contains the Catalog. 
      </meta>
      
      <id name = "product_id" type = "int" column = "product_id">
         <generator class="native"/>
      </id>
      
      <property name = "product_name" column = "product_name" type = "string"/>
      <property name = "brand" column = "brand" type = "string"/>
      <property name = "description" column = "description" type = "string"/>
      <property name = "price" column = "price" type = "float"/>
      <property name = "quantity" column = "quantity" type = "int"/>
      <property name = "pic_location" column = "pic_location" type = "string"/>
     
	</class>
	
</hibernate-mapping>